{
	"name": "nuxt-snippets",
	"displayName": "Nuxt VSCode Snippets",
	"description": "Snippets for vue/nuxt projects",
	"icon": "images/logo.png",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/praburangki/nuxt-vscode-snippets.git"
	},
	"scripts": {
